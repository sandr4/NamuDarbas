						{
							"listen": "test",
							"script": {
								"exec": [
									"// example using pm.response.to.be*\r",
									"pm.test(\"It should check if response is valid and have a body\", function () {\r",
									"    // assert that the status code is 200\r",
									"    pm.response.to.be.nested; // info, success, redirection, clientError,  serverError, are other variants\r",
									"    // assert that the response has a valid JSON body\r",
									"    pm.response.to.be.withBody;\r",
									"    pm.response.to.be.json; // this assertion also checks if a body  exists, so the above check is not needed\r",
									"});\r",
									"\r",
									"pm.test(\"It should successfully create a new user\", function () {\r",
									"    const token = pm.environment.get(\"auth\");\r",
									"    console.log(token);\r",
									"    let date = Math.round((new Date()).getTime() / 1000 + 200);\r",
									"    let email = 'POSTrecord.' + date + '@email.com';\r",
									"    //const email = pm.environment.get(email);\r",
									"    console.log(email);\r",
									"    const name = pm.environment.get('user_name');\r",
									"    const gender = pm.environment.get('user_gender');\r",
									"    const status = pm.environment.get('user_status');\r",
									"    const newUser = { name, email, gender, status }\r",
									"    const postRequest = {\r",
									"        url: 'https://gorest.co.in/public/v1/users',\r",
									"        method: 'POST',\r",
									"        json: true,\r",
									"        header: {\r",
									"            'Content-Type': 'application/json',\r",
									"            Authorization: token\r",
									"        },\r",
									"        body: JSON.stringify(newUser)\r",
									"    }\r",
									"    pm.sendRequest(postRequest, (err, response) => {\r",
									"        if (err) {\r",
									"            console.log(err);\r",
									"        }\r",
									"        else {\r",
									"            const data = response.json().data;\r",
									"            pm.expect(response.status).to.be.equal('Created');\r",
									"            pm.expect(response.code).to.be.equal(201);\r",
									"            //pm.expect(response.data).to.equals(newUser);\r",
									"            console.log(data);\r",
									"            const createdUser = {\r",
									"                name: data.name,\r",
									"                email: data.email,\r",
									"                gender: data.gender,\r",
									"                status: data.status\r",
									"            }\r",
									"            console.log(newUser, createdUser);\r",
									"            pm.expect(newUser).to.deep.equal(createdUser);\r",
									"        }\r",
									"    });\r",
									"});\r",
									"\r",
									"// parsing inputs\r",
									"pm.test(\"It should return message if Email is invalid\", function () {\r",
									"    const response = pm.response.json();\r",
									"    const data = response.data;\r",
									"    pm.expect(pm.response.status).to.be.equal(\"Unprocessable Entity\");\r",
									"    pm.expect(pm.response.code).to.be.equal(422);\r",
									"    pm.expect(data[0].field).to.be.equal(\"email\");\r",
									"    pm.expect(data[0].message).to.be.equal(\"is invalid\");\r",
									"});\r",
									"\r",
									"// var i = 0;\r",
									"// while (i < 3) {\r",
									"//     postman.setNextRequest(\"Get user\");\r",
									"//     console.log(\"test iteration \" + i);\r",
									"//     i++;\r",
									"// }"
								],
